{
        "tz": "Europe/Paris",
        "title": {
                "text": "FILEBROWSER",
                "url": "https://www.google.com",
                "image": "images/mypic.png"
        },
        "bookmarks": {
                "enabled": 1,
                "max": 15
        },
        "auth": "ldap",
        "ldap": {
                "server": "your.ldap.server",
                "port": 389,
                "pattern": "cn={login},ou=DAP,ou=DOP,dc=xxxx,dc=xxxx",
                "protocol_version": 3
        },
        "display": {
                "columns": [ "Filename", "Size", "Perms", "mtime",  "type", "actions" ],
                "pagesize": 15
        },
        "csv": {
                "enabled": 1,
                "columns": [ "Filename", "Size", "mtime", "ctime", "type" ]
        },
        "volumes": [
                {
                "name": "/tmp",
                "path": "/tmp",
                "download": "yes",
                "delete": "yes",
                "upload": "yes",
                "showhiddenfiles": "yes"
                },
                {
                "name": "Home",
                "path": "/home",
                "download": "yes",
                "delete": "no",
                "upload": "no",
                "encoding": "iso-8859-1"
                }
        ],
        "default": "/tmp"
}
