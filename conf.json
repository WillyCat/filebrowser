{
        "bookmarks": {
                "enabled": 1,
                "max": 15
        },
        "ldap": {
                "server": "xxxxxxx",
                "port": 389,
                "pattern": "cn={login},ou=DAP,ou=DOP,dc=xxxx,dc=xxxx",
                "protocol_version": 3
        },
        "display": {
                "columns": [ "Filename", "Size", "Perms", "mtime",  "type", "actions" ],
                "pagesize": 15
        },
        "csv": {
                "enabled": 1,
                "columns": [ "Filename", "Size", "mtime", "ctime", "type" ]
        },
        "volumes": [
                {
                "name": "/tmp",
                "path": "/tmp",
                "download": 1,
                "delete": 1,
                "upload": 1
                },
                {
                "name": "Home",
                "path": "/home",
                "download": 1,
                "delete": 0,
                "upload": 0,
                "encoding": "iso-8859-1"
                }
        ],
        "default": "/tmp"
}
